<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    //Tab

    <TextView
        android:id="@+id/bar"
        android:layout_width="412dp"
        android:layout_height="43dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="0dp"
        android:foreground="@color/light" />


    <TextView
        android:id="@+id/username"
        android:layout_width="109dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/bar"
        android:layout_alignParentStart="true"
        android:layout_marginStart="160dp"
        android:layout_marginTop="9dp"
        android:fontFamily="@font/rubik_medium"
        android:text="USERNAME"
        android:textColor="@color/green"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/bntOrders"
        android:layout_width="match_parent"
        android:layout_height="59dp"
        android:layout_below="@+id/bar"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="0dp"
        android:background="@drawable/square"
        android:fontFamily="@font/rubik_light"
        android:padding="20dp"
        android:text="@string/orders"
        android:textColor="@color/black"
        android:textSize="14dp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="23dp"
        android:layout_height="29dp"
        android:layout_below="@+id/bar"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="33dp"
        app:srcCompat="@drawable/ic_carrinho" />

    <TextView
        android:id="@+id/bntPoints"
        android:layout_width="match_parent"
        android:layout_height="59dp"
        android:layout_below="@+id/bntOrders"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="-3dp"
        android:layout_marginEnd="0dp"
        android:background="@drawable/square"
        android:fontFamily="@font/rubik_light"
        android:padding="20dp"
        android:text="@string/points"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="14dp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="27dp"
        android:layout_height="31dp"
        android:layout_alignTop="@+id/bntPoints"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="33dp"
        app:srcCompat="@drawable/ic_coins" />

    <TextView
        android:id="@+id/bntDetails"
        android:layout_width="match_parent"
        android:layout_height="59dp"
        android:layout_below="@+id/bntPoints"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="-3dp"
        android:layout_marginEnd="0dp"
        android:background="@drawable/square"
        android:fontFamily="@font/rubik_light"
        android:padding="20dp"
        android:text="@string/details"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="14dp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="22dp"
        android:layout_height="23dp"
        android:layout_below="@+id/bntPoints"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="33dp"
        app:srcCompat="@drawable/ic_user" />

    <TextView
        android:id="@+id/bntOut"
        android:layout_width="match_parent"
        android:layout_height="59dp"
        android:layout_below="@+id/bntDetails"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="-3dp"
        android:layout_marginEnd="0dp"
        android:background="@drawable/square"
        android:fontFamily="@font/rubik_light"
        android:padding="20dp"
        android:text="@string/out"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="14dp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="25dp"
        android:layout_height="27dp"
        android:layout_below="@+id/bntDetails"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="33dp"
        app:srcCompat="@drawable/ic_out" />


</RelativeLayout>